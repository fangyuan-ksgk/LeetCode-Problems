   "source": [
    "def circuit(gas, cost):\n",
    "    m = 0\n",
    "    n = len(gas)\n",
    "    mlist = [0]*n\n",
    "    for i in range(n-1,-1,-1):\n",
    "        m+=(gas[i]-cost[i])\n",
    "        mlist[i]=m\n",
    "    if mlist[0]>=0:\n",
    "        return mlist.index(max(mlist))\n",
    "    else:\n",
    "        return -1"
   ]
